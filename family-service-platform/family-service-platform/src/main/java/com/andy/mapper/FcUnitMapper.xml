<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.andy.mapper.FcUnitMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.andy.bean.FcUnit">
        <id column="id" property="id" />
        <result column="building_code" property="buildingCode" />
        <result column="unit_code" property="unitCode" />
        <result column="unit_name" property="unitName" />
        <result column="start_floor" property="startFloor" />
        <result column="end_floor" property="endFloor" />
        <result column="start_cell_id" property="startCellId" />
        <result column="stop_cell_id" property="stopCellId" />
        <result column="remark" property="remark" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, building_code, unit_code, unit_name, start_floor, end_floor, start_cell_id, stop_cell_id, remark
    </sql>

</mapper>
